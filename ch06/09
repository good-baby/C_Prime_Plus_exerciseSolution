#include <stdio.h>
#include "string.h"
void outcome(float ,float );
int main(){
    float a,b;

    printf("enter two float:");
    scanf("%f %f",&a,&b);

    do {
        outcome(a,b);
        printf("enter two float:");
    } while ( scanf("%f %f",&a,&b));
    printf("done");

}
void outcome(float a,float b){
    printf("outcome:%f\n",(a-b)/(a*b));
}
