#include <stdio.h>
#include "stdbool.h"

int main(){
    int n_space,n_linenotation,n_other;
    n_linenotation=n_other=n_space=0;
    char ch;
    while((ch=getchar())!='#'){
        if(ch==' ')
            n_space++;
        else if(ch=='\n')
            n_linenotation++;
        else
            n_other++;
    }
    
    // 在此处可以输出统计结果或进行其他操作

}
